{
  "name": "@prodigy/polybooljs",
  "version": "1.0.4",
  "description": "Boolean operations on polygons (union, intersection, difference, xor)",
  "main": "index.js",
  "scripts": {
    "build": "browserify index.js -o dist/polybool.js && uglifyjs dist/polybool.js --comments > dist/polybool.min.js",
